<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JNITestActivity">

    <Button
        android:id="@+id/bt_ini_test1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="59dp"
        android:text="test jni interface"
        app:layout_constraintBottom_toTopOf="@+id/bt_jni_test2"
        app:layout_constraintStart_toStartOf="@+id/bt_jni_test2" />

    <Button
        android:id="@+id/bt_jni_test2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="291dp"
        android:text="test passing parameters"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bt_jni_test3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:text="boolean test"
        app:layout_constraintEnd_toEndOf="@+id/bt_jni_test2"
        app:layout_constraintTop_toBottomOf="@+id/bt_jni_test2" />

    <Button
        android:id="@+id/bt_jni_test4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:text="string test"
        app:layout_constraintEnd_toEndOf="@+id/bt_jni_test3"
        app:layout_constraintTop_toBottomOf="@+id/bt_jni_test3" />
</androidx.constraintlayout.widget.ConstraintLayout>